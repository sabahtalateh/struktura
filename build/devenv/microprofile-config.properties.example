# Microprofile server properties
server.port=8081
server.host=0.0.0.0

# Change the following to true to enable the optional MicroProfile Metrics REST.request metrics
metrics.rest-request.enabled=false

# Application properties. This is the default greeting
app.greeting=Hello

# Datasource properties
javax.sql.DataSource.master.dataSourceClassName=org.postgresql.ds.PGSimpleDataSource
javax.sql.DataSource.master.dataSource.url=jdbc:postgresql://127.0.0.1:54321/struktura
javax.sql.DataSource.master.dataSource.user=struktura
javax.sql.DataSource.master.dataSource.password=struktura
